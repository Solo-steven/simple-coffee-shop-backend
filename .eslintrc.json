{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "no-await-in-loop": "off",
        "no-underscore-dangle": "off",
        "new-cap": "off",
        "no-restricted-syntax": "off"
    }
}
